{
  "name": "classify-imports-extensions",
  "version": "1.0.1",
  "displayName": "Classify Imports",
  "description": "VScode extensions to classify imports into external, internal, and unknown categories.",
  "author": "rinonguci",
  "repository": {
    "type": "git",
    "url": "https://github.com/rinonguci/classify-imports-extensions.git"
  },
  "license": "MIT",
  "keywords": [
    "VSCode",
    "extension",
    "imports",
    "classification",
    "sort-imports"
  ],
  "homepage": "https://github.com/rinonguci/classify-imports-extensions#readme",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.88.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "classify-imports-extensions.sortImports",
        "title": "Classify Imports: Sort Imports"
      }
    ],
    "configuration": {
      "optionsDefault": {
        "type": "object",
        "properties": {
          "importOrder": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "importOrderSeparation": {
            "type": "boolean"
          },
          "importOrderSortByLength": {
            "type": "boolean"
          },
          "importOrderSplitType": {
            "type": "boolean"
          },
          "importWithSemicolon": {
            "type": "boolean"
          },
          "importOrderAddComments": {
            "type": "boolean"
          },
          "additionalOption1": {
            "type": "boolean",
            "default": true,
            "description": "Description of additional option 1"
          },
          "additionalOption2": {
            "type": "string",
            "default": "default value",
            "description": "Description of additional option 2"
          }
        },
        "default": {
          "importOrder": [
            "<THIRD_PARTY_MODULES> --comment THIRD PARTY MODULES",
            "<RELATIVE_MODULES> --comment RELATIVE MODULES",
            "<TYPES_MODULES> --comment TYPES MODULES"
          ],
          "importOrderSeparation": true,
          "importOrderSortByLength": true,
          "importOrderSplitType": true,
          "importWithSemicolon": true,
          "importOrderAddComments": true,
          "additionalOption1": true,
          "additionalOption2": "default value"
        },
        "description": "Default options for the extension"
      }
    }
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel-generator": "^6.25.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/parser": "^7.24.4",
    "@babel/types": "^7.24.0",
    "babel-generator": "^6.26.1"
  }
}
